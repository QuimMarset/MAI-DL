{
    "optimizer": "Adam",
    "lr": 0.0005,
    "lr_decay": true,
    "decay_epochs": 2.5,
    "decay_rate": 0.95,
    "weight_decay": 0,
    "model": "CNN-Residuals",
    "kernel_sizes": [
        3
    ],
    "conv_filters": [
        16,
        32,
        64,
        98,
        128
    ],
    "pooling_type": "Max",
    "padding": "same",
    "dense_units": [
        256
    ],
    "dropout": 0.15,
    "batch_norm": true,
    "activation": "gelu",
    "augmentation": true,
    "batch_size": 32,
    "train_normalization": false,
    "epochs": 100,
    "early_stopping": false,
    "patience": 10,
    "min_delta": 0.0001,
    "label_smoothing": 0.5,
    "seed": true,
    "global_seed": 42,
    "operational_seed": 42
}